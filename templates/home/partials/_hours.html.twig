<section id="opening-hours-section" class="container mt-6 mb-6">
	<div class="sect-horaires">
		<h2 class="text-center">Horaires d'ouverture</h2>
		<div class="row">
			<div class="col-md-6 col-lg-4">
				<div class="card mb-3">
					<div class="card-body">
					{% for hour in hours %}
						
					<p class="fw-bold">{{ hour.day }}</p> - {{ hour.startDate|date('H:i') }} / {{ hour.endDate|date('H:i') }}
					{% endfor %}
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
